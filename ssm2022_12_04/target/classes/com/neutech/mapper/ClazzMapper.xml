<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neutech.mapper.ClazzMapper">
     <resultMap id="Base" type="com.neutech.entity.Clazz">
         <id property="id" column="id"/>
         <result property="name" column="name"/>
         <result property="room" column="room"/>
     </resultMap>
     <select id="list" resultMap="Base">
         select *from s_clazz;
     </select>
    <delete id="delById">
        delete from s_clazz where id = #{id};
    </delete>
    <insert id="insert">
        insert into s_clazz(name,room)
            values(#{name},#{room});
    </insert>
    <select id="selectById" resultMap="Base">
        select name,room from s_clazz where id = #{id};
    </select>
    <update id="updateById">
        update s_clazz set name = #{name},room = #{room} where id = #{id};
    </update>
</mapper>